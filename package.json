{
  "name": "my-app",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "12.1.10",
    "@types/jest": "26.0.15",
    "@types/material-ui": "0.21.9",
    "@types/node": "12.0.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "aws-amplify": "^4.3.3",
    "axios": "0.21.1",
    "axios-mock-adapter": "1.19.0",
    "axios-mock-adapter-path-params": "1.20.0",
    "csv-parser": "3.0.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "faker": "5.5.3",
    "lint-staged": "11.1.2",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "precommit-hook": "3.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "typescript": "4.1.2",
    "uuid": "8.3.2",
    "web-vitals": "1.0.1",
    "yarn-run-all": "3.1.1"
  },
  "scripts": {
    "start": "HTTPS=true SSL_CRT_FILE=_wildcard.aspire.com.pem SSL_KEY_FILE=_wildcard.aspire.com-key.pem react-scripts start",
    "build": "REACT_APP_BUILD_ENV=localhost react-scripts build",
    "build-dev": "REACT_APP_BUILD_ENV=dev react-scripts build",
    "build-demo": "REACT_APP_BUILD_ENV=demo react-scripts build",
    "build-staging": "REACT_APP_BUILD_ENV=staging react-scripts build",
    "build-prod": "REACT_APP_BUILD_ENV=prod react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"**/*.{ts,tsx}\"",
    "validate": "npm-run-all lint"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.3.6",
    "@storybook/addon-essentials": "6.3.6",
    "@storybook/addon-links": "6.3.6",
    "@storybook/node-logger": "6.3.6",
    "@storybook/preset-create-react-app": "3.2.0",
    "@storybook/react": "6.3.6",
    "@types/axios": "0.14.0",
    "@types/axios-mock-adapter": "1.10.0",
    "@types/faker": "5.5.8",
    "@types/lodash": "4.14.172",
    "@types/moment": "^2.13.0",
    "@types/react-helmet": "^6.1.4",
    "@types/react-router-dom": "5.1.8",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0"
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  }
}
